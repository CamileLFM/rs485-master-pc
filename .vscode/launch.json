{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug rs485 (gdb)",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/rs485",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "externalConsole": true,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
          { "description": "Enable pretty-printing", "text": "-enable-pretty-printing", "ignoreFailures": true }
        ],
        "preLaunchTask": "C: build (Linux)"
      }
    ]
  }
  